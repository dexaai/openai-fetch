{
  "name": "openai-fetch",
  "version": "2.0.0-beta.1",
  "description": "OpenAI client powered by fetch",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": "dexaai/openai-fetch",
  "author": {
    "name": "Riley Tomasek",
    "email": "hi@rile.yt",
    "url": "https://rile.yt"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.dist.json",
    "clean": "rm -rf dist node_modules",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint src",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint && yarn typecheck",
    "release": "np",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "private": false,
  "dependencies": {
    "ky": "^1.1.0"
  },
  "devDependencies": {
    "@dexaai/eslint-config": "^0.4.0",
    "eslint": "^8.52.0",
    "np": "^8.0.4",
    "openai": "^4.12.4",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "prettier": {
    "singleQuote": true
  },
  "engines": {
    "node": ">=18"
  }
}
